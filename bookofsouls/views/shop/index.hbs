{{# each products }}
<div class="row">
    {{# each this }}
    <div class="col-sm-6 col-md-4">
        <div class="thumbnail">
            <img src="{{ this.imagePath }}" class=img-responsive>
            <div class="caption">
                <h3>Thumbnail label</h3>
                <p class="description"> O Senhor dos Anéis é uma trilogia de livros de alta fantasia escrita pelo
                    escritor britânico J. R. R. Tolkien
                </p>
                <div class="clearfix">
                    <div class="price pull-left"> R$ 59.90 </div>
                    <a href="#" class="btn btn-success pull-right" role="button">Button</a>
                </div>
            </div>
        </div>
    </div>
    {{/each}}
</div>
{{/each}}

